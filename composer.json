{
    "name": "ivoschimani/contao-youtube-sync",
    "description": "Contao extension to synchronize a contao news archive with a YouTube playlist.",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/ivoschimani/contao-youtube-sync",
    "authors": [
        {
            "name": "Ivo Schimani",
            "email": "ivo@schimani.de",
            "role": "Developer"
        }
    ],
	"support": {
		"source": "https://github.com/ivoschimani/contao-youtube-sync",
		"forum": "https://community.contao.org/de"
	},
	"funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=7.1",
        "contao/core-bundle": "^4.9 || ^5.0",
        "contao/news-bundle": "^4.9 || ^5.0",
        "doctrine/dbal": "^2.11 || ^3.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "symfony/config": "^4.4 || ^5.1 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.1 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.1 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.1 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.1 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoYouTubeSync\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoYouTubeSync\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0"
    }
}
